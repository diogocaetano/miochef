<div class="row">
  <div class="col-lg-12">
    <a href="/<%= plural_table_name %>/new">
        <button type="button" class="btn btn-primary">Adicionar <%= human_name %></button>
    </a>
    <div class="panel"></div>
    <section class="panel">
      <header class="panel-heading">
          <%= plural_table_name.titleize %>
      </header>
      <div class="panel-body">
        <section id="unseen">
          <table class="table table-bmoduleed table-striped table-condensed">
            <thead>
              <tr>
                <% attributes.reject(&:password_digest?).each do |attribute| -%>
                  <th><%= attribute.human_name %></th>
                <% end -%>
                <th colspan='3'>Ações</th>
              </tr>
            </thead>
            <tbody>
              <%% @<%= plural_table_name %>.each do |<%= singular_table_name %>| %>
                <tr>
                  <% attributes.reject(&:password_digest?).each do |attribute| -%>
                    <td><%%= <%= singular_table_name %>.<%= attribute.name %> %></td>
                  <% end -%>
                  <td>
                    <%%= link_to raw("<i class='fa fa-eye'></i>"), <%= singular_table_name %>, :<%= "class => 'btn btn-primary'"%> %>
                    <%%= link_to raw("<i class='fa fa-pencil'></i>"), edit_<%= singular_table_name %>_path(<%= singular_table_name %>), :<%= "class => 'btn btn-info'"%> %>
                    <%%= link_to raw("<i class='fa fa-trash-o'></i>"), <%= "'#delete_"%><%=singular_table_name %><%="_'"%>+<%=singular_table_name %><%=".id.to_s"%>, :<%= "class => 'btn btn-danger'"%>, :<%= "'data-toggle' => 'modal'"%>  %>
                  </td>
                </tr>
                <div class="modal fade modal-dialog-center in" id="<%= "delete_" + singular_table_name + "_" %><%%= <%=singular_table_name + ".id.to_s"%> %>"
                  tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;"><div class="modal-backdrop fade in" style="height: 749px;">
                </div>
                <div class="modal-dialog ">
                  <div class="modal-content-wrap">
                    <div class="modal-content">
                      <div class="modal-header">
                          <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
                          <h4 class="modal-title">Remover <%= human_name.titleize %></h4>
                      </div>
                      <div class="modal-body">
                        <div class="text-center">
                          Deseja Realmente Remover o <%= human_name.titleize %>
                        </div>
                      </div>
                      <div class="modal-footer">
                        <div class="pull-left">
                          <button data-dismiss="modal" class="btn btn-default" type="button">Fechar</button>
                        </div>
                        <%%= link_to raw("Confirmar"), <%= singular_table_name %>, :<%= "class => 'btn btn-primary'"%>, :<%= "'data-method' => 'delete'"%> %>
                      </div>
                    </div>
                  </div>
                </div>
              <%% end -%>
            </tbody>
          </table>
        </section>
      </div>
    </section>
  </div>
</div>