<div class="col-lg-12">
  <%= form_for(@window_request) do |f| %>  
  <%= link_to raw("<button type='button' class='btn btn-default'>Voltar</button>"), window_requests_path %>
  <div class="panel"></div>
  <section class="panel">
    <header class="panel-heading">
       Window_requests
    </header>
    <% if @window_request.errors.any? %>
      <div class="alert alert-danger">
        <% @window_request.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </div>
    <% end %>
    <div class="panel-body">
                  <div class="form-group">
            <%= f.label :initial_time %><br>
            <%= f.time_select :initial_time, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :final_time %><br>
            <%= f.time_select :final_time, :class => 'form-control' %>
          </div>
            </div>
  </section>
  <div class="form-group">
      <%= f.submit 'Salvar', :class => 'btn btn-primary' %>
  </div>
  <% end -%>
</div>