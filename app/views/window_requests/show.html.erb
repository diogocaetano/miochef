<div class="col-lg-12">  
  <%= link_to raw("<button type='button' class='btn btn-default'>Voltar</button>"), window_requests_path %>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    		      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Horário Inicial</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @window_request.initial_time.to_s(:time) %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Horário Final</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @window_request.final_time.to_s(:time) %></label>
          </div>
	      </div>
	        </div>
  </section>
</div>