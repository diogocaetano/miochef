<div class="col-lg-12">
  <a href="/admin/chefs">
      <button type="button" class="btn btn-default">Voltar</button>
  </a>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    		<div class="form-group">
          <label for="number" class="control-label col-lg-3">Tipo do Chefe</label>
          <div class="col-lg-9">
            <label class="form-control-static"><%= @chef.chef_type ? @chef.chef_type.name : 'Não cadastrado' %></label>
          </div>
        </div>
        <div class="form-group">
          <label for="number" class="control-label col-lg-3">Nome</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.name %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Descrição</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.description %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">CPF</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.cpf %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">RG</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.rg %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Data de Nascimento</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= I18n.l(@chef.born_date) %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">País</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.country.name %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Email</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.email %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Telefone 1</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.phone_one %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Telefone 2</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.phone_two %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Especialidade</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.specialty %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Naturalidade</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.naturalness %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Formação Acadêmica</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.academic_education %></label>
          </div>
	      </div>
	    	<div class="form-group">
          <label for="number" class="control-label col-lg-3">Universidade</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.university %></label>
          </div>
	      </div>

        <div class="form-group">
          <label for="number" class="control-label col-lg-3">Badges</label>
          <div class="col-lg-9">
            <% @chef.badges.collect do |b| %>
              <%= image_tag b.image.url(:thumb) %>
            <% end %>
          </div>
        </div>
        <div class="form-group">
          <label for="number" class="control-label col-lg-3">Ativo?</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @chef.active? %></label>
          </div>
        </div>
	        </div>
  </section>
</div>