<div class="col-lg-12">
  <a href="/nutritional_tables">
      <button type="button" class="btn btn-default">Voltar</button>
  </a>
  <div class="panel"></div>
  <section class="panel">
    <% if @nutritional_table.errors.any? %>
      <div class="alert alert-danger">
        <% @nutritional_table.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </div>
    <% end %>
    <div class="panel-body">
      <%= form_for(@nutritional_table) do |f| %>
                  <div class="form-group">
            <%= f.label :energetic_value %><br>
            <%= f.text_field :energetic_value, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :carbohydrate %><br>
            <%= f.text_field :carbohydrate, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :protein %><br>
            <%= f.text_field :protein, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :total_fat %><br>
            <%= f.text_field :total_fat, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :saturated_fat %><br>
            <%= f.text_field :saturated_fat, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :trans_fat %><br>
            <%= f.text_field :trans_fat, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :sodium %><br>
            <%= f.text_field :sodium, :class => 'form-control' %>
          </div>
                  <div class="form-group">
            <%= f.label :iron %><br>
            <%= f.text_field :iron, :class => 'form-control' %>
          </div>
                <div class="form-group">
            <%= f.submit 'Salvar', :class => 'btn btn-primary' %>
        </div>
      <% end -%>
    </div>
  </section>
</div>