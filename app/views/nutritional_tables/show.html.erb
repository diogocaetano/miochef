<div class="col-lg-12">
  <%= link_to raw("<button type='button' class='btn btn-default'>Voltar</button>"), nutritional_tables_path %>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    		      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Energetic value</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.energetic_value %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Carbohydrate</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.carbohydrate %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Protein</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.protein %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Total fat</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.total_fat %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Saturated fat</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.saturated_fat %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Trans fat</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.trans_fat %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Sodium</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.sodium %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Iron</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @nutritional_table.iron %></label>
          </div>
	      </div>
	        </div>
  </section>
</div>