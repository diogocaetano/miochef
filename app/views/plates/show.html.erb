          <label for="number" class="control-label col-lg-3">Ingredients</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.ingredients %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Plate accompaniment</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.plate_accompaniment %></label>
          </div>
	      </div>
