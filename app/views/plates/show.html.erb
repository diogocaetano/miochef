          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.chef %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Plate type</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.plate_type %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Plate badge</label>
