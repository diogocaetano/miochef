<div class="col-lg-12">
  <a href="/plates">
      <button type="button" class="btn btn-default">Voltar</button>
  </a>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    		      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Title</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.title %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Description</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.description %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Price</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.price %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Available quantity</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.available_quantity %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Photo</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.photo %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Active</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @plate.active %></label>
          </div>
	      </div>
	    	      <div class="form-group">
