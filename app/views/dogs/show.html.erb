<div class="col-lg-12">  
  <%= link_to raw("<button type='button' class='btn btn-default'>Voltar</button>"), dogs_path %>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    	
	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Nome</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @dog.nome %></label>
          </div>
	      </div>
	    
	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Age</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @dog.age %></label>
          </div>
	      </div>
	    
    </div>
  </section>
</div>