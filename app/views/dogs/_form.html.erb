<div class="col-lg-12">
  <%= form_for(@dog) do |f| %>  
  <%= link_to raw("<button type='button' class='btn btn-default'>Voltar</button>"), dogs_path %>
  <div class="panel"></div>
  <section class="panel">
    <header class="panel-heading">
       Dogs
    </header>
    <% if @dog.errors.any? %>
      <div class="alert alert-danger">
        <% @dog.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </div>
    <% end %>
    <div class="panel-body">
        
          <div class="form-group">
            <%= f.label :nome %><br>
            <%= f.text_field :nome, :class => 'form-control' %>
          </div>
        
          <div class="form-group">
            <%= f.label :age %><br>
            <%= f.number_field :age, :class => 'form-control' %>
          </div>
        
    </div>
  </section>
  <div class="form-group">
      <%= f.submit 'Salvar', :class => 'btn btn-primary' %>
  </div>
  <% end -%>
</div>