<div class="col-lg-12">
    <a href="/admin/permissions">
        <button type="button" class="btn btn-default">Voltar</button>
    </a>
    <div class="panel"></div>
  <section class="panel">
      <div class="panel-heading">
          <div class="panel-title">Detalhes</div>
      </div>
      <div class="panel-body form-horizontal">
          <div class="form-group">
              <label for="number" class="control-label col-lg-3">Módulo</label>
              <div class="col-lg-9">
                  <label for="number" class="form-control-static"><%= @permission.entity.name %></label>
              </div>
          </div>
          <div class="form-group">
              <label for="number" class="control-label col-lg-3">Nome da Ação</label>
              <div class="col-lg-9">
                  <label for="number" class="form-control-static"><%= @permission.action_name %></label>
              </div>
          </div>
          <div class="form-group">
              <label for="number" class="control-label col-lg-3">Ação</label>
              <div class="col-lg-9">
                  <label for="number" class="form-control-static"><%= @permission.action %></label>
              </div>
          </div>
      </div>
  </section>
</div>