<div class="col-lg-12">  
  <%= link_to raw("<button type='button' class='btn btn-default'>Voltar</button>"), clients_path %>
  <div class="panel"></div>
	<section class="panel">
    <div class="panel-heading">
        <div class="panel-title">Detalhes</div>
    </div>
    <div class="panel-body form-horizontal">
    		      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Name</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @client.name %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Last name</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @client.last_name %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Email</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @client.email %></label>
          </div>
	      </div>
	    	      <div class="form-group">
          <label for="number" class="control-label col-lg-3">Cep</label>
          <div class="col-lg-9">
            <label for="number" class="form-control-static"><%= @client.cep %></label>
          </div>
	      </div>
	        </div>
  </section>
</div>