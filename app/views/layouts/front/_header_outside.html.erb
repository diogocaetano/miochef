<header id="header-outside">
  <div class="container-fluid int">
    <div id="active-menu-responsive" class="visible-sm visible-xs hidden-md hidden-lg">
      <i class="fa fa-bars" aria-hidden="true"></i>
    </div>
    <div class="row">
      <div class="col-md-4 col-sm-4 hidden-xs hidden-sm visible-md visible-lg">
        <div class="menu-int left">
          <%= render 'layouts/front/menu_links_outside' %>
        </div>
      </div>
      <div id="wrap-menu-logo-int" class="col-md-4 col-sm-12">
        <h1>
          <%= link_to home_path, :class => "" do %>
          <%= image_tag 'content/miochef.png', alt: "Miochef" %>
          <% end %>
          <%= link_to home_path, :class => "hidden-md hidden-lg" do %>
          <%= image_tag 'content/miochef-name.png', alt: "Miochef" %>
          <% end %>
        </h1>
      </div><!-- wrap-menu-logo-int -->
      <div class="col-md-4 col-sm-4 hidden-xs hidden-sm visible-md visible-lg">
        <div class="menu-int right">
          <%= render 'layouts/front/menu_links_outside' %>
        </div>
      </div>
    </div><!-- row -->
  </div><!-- container-fluid -->
</header>
